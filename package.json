    "@principal-ai/code-city-react": "^0.1.3",
    "@principal-ai/code-city-server": "^0.1.0",
    "@principal-ai/repository-abstraction": "^0.1.0",
    "@types/gifencoder": "^2.0.3",
    "a24z-memory": "^0.5.52",
    "canvas": "^3.1.2",
    "gifencoder": "^2.0.1",
    "hast-util-sanitize": "^5.0.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.534.0",
    "mermaid": "^11.6.0",
    "next": "15.2.3",
    "next-auth": "^4.24.7",
    "puppeteer-core": "^24.18.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "themed-markdown": "^0.1.15",
    "uuid": "^11.1.0",
    "ws": "^8.18.0",
    "zod": "^4.0.14"
