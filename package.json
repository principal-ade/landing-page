  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint-config-next": "15.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.5",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.6",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vite": "^7.1.5"
  }
